{
    "name": "producttrap/driver-woolworths-australia",
    "type": "library",
    "description": "A Woolworths Australia driver for ProductTrap",
    "license": "MIT",
    "keywords": [
        "producttrap-driver",
        "woolworths",
        "australia"
    ],
    "require": {
        "php": "^8.1",
        "graham-campbell/guzzle-factory": "^5.0",
        "producttrap/browser": "dev-main",
        "producttrap/producttrap": "^1.0",
        "symfony/css-selector": "^6.1"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "orchestra/testbench": "^7.9",
        "pestphp/pest": "^1.21.1",
        "phpstan/phpstan": "^1.4.5",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ProductTrap\\WoolworthsAustralia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProductTrap\\WoolworthsAustralia\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test:types": "phpstan analyse --ansi --memory-limit=-1",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:types",
            "@test:unit"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProductTrap\\WoolworthsAustralia\\WoolworthsAustraliaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../producttrap-browser"
        },
        {
            "type": "path",
            "url": "../producttrap"
        }
    ]
}
